{
  "manifest_version": 3,
  "name": "1234",
  "version": "1.0.0",
  "description": "A Chrome extension that adds a countdown to any video",
  "author": {
    "email": "mathfrmorais@gmail.com"
  },
  "icons": {
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "./html/index.html"
  }
}
